<%- include('partials/header',{testDetail}) %>
<%- include('partials/SideBar',{testDetail}) %>

<div class="content">
    <div class="row">
      <div class="col-md-4">
      </div>
    
      <div class="col-md-8">
      <form class="" method="POST" action="/createtest" enctype="multipart/form-data">
        <div class="card card-user">
          <div class="card-header">
            <h5 class="card-title">Create Test</h5>
          </div>
          <div class="card-body">
            <form>
              <div class="row">
                <div class="col-md-5 pr-1">
                  <div class="form-group">
                    <label>Student Email</label>
                    <input id="" name="id" type="text" class="form-control workExperience" placeholder="" value="<%= testDetail?.email %>" >
           
                  </div>
                </div>
                <div class="col-md-4 pl-1">
                  <div class="form-group">
                    <label for="exampleInputEmail1">Student Name</label>
                    <input type="text" name="name" class="form-control" placeholder="" value="<%= testDetail?.name %>">
                  </div>
                </div>
              </div>
              <div class="row">
                <div class="col-md-6 pr-1">
                  <div class="form-group">
                    <label>Teacher Name</label>
                    <input id="firstName" name="tname" type="text" class="form-control" placeholder="" value="<%= testDetail?.tname %>">
                  </div>
                </div>
                <div class="col-md-6 pl-1">
                  <div class="form-group">
                    <label>Teacher Email</label>
                    <input id="lastName" type="text" name="temail" class="form-control" placeholder="" value="<%= testDetail?.temail %>">
                  </div>
                </div>
              </div>
              <div class="row">
                <div class="col-md-5 pr-1">
                    <div class="form-group">
                      <label>Test ID</label>
                      <input id="" name="id" type="text" class="form-control workExperience" placeholder="" value="<%= testDetail?.id %>" >
                    </div>
                  </div>
                <div class="col-md-4 px-1">
                  <div class="form-group">
                    <label>Submission Date</label>
                    <input type="date" name="date" class="form-control" placeholder="" value="<%= testDetail?.submitDate %>">
                  </div>
                </div>
              </div>
              <div class="col-md-4 px-1">
                <div class="form-group">
                  <label>Total Marks</label>
                  <input type="number" name="Marks" class="form-control" placeholder="" value="<%= testDetail?.totalMarks %>">
                </div>
              </div>
              <div class="row">
                <div class="update ml-auto mr-auto">
                  <button type="submit" class="btn btn-primary btn-round">Submit</button>
                </div>
              </div>
            </form>
          </div>
        </div>
      </div>
    </form>
    </div>
  </div>


<%- include('partials/footer') %>